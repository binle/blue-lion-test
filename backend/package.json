{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bach Le Xuan - Bin Le",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias",
    "start": "rm -rf dist && tsc && tsc-alias && node ./dist/index.js",
    "wb": "concurrently \"nodemon --config nodemon.build.json\"",
    "wr": "concurrently \"nodemon --config nodemon.run.json\"",
    "forever": "npm run build && forever -s start ./dist/index.js",
    "watch": " npm run build && concurrently \"npm run wb\" \"npm run wr\" ",
    "start-ts": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch-ts": "concurrently \"nodemon --config nodemon.run-ts.json\""
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^14.14.8",
    "@types/pg": "^8.6.5",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.3.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@bakku/logger": "^3.0.0",
    "@bakku/platform": "^3.0.0",
    "axios": "^0.21.1",
    "fs-extra": "^9.0.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "redis": "^4.2.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.1"
  }
}
